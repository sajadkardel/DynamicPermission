@model DynamicPermission.AspNetCore.ViewModels.AppSetting.RoleValidationGuidViewModel
@{
    ViewData["Title"] = "RoleValidationGuid";
}

<h1>RoleValidationGuid</h1>

<form asp-controller="AppSetting" asp-action="RoleValidationGuid" method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="form-group">
        <label class="control-label" asp-for="@Model.Value"></label>
        <input readonly="readonly" class="form-control" asp-for="@Model.Value" />
    </div>
    <div class="form-group">
        <label class="control-label" asp-for="@Model.LastTimeChanged"></label>
        <input readonly="readonly" class="form-control" asp-for="@Model.LastTimeChanged" />
    </div>

    <button class="btn btn-primary" type="submit">Generate New Guid</button>
</form>